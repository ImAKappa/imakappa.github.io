:root{--bg-main:#121212;--surface-0:#2c2c2c;--surface-1:#313131;--text-colour:#ffffff;--text-colour-sub:rgba(255, 255, 255, 0.7);--green-50:#2ecc71;--green-75:#28b463;--green-100:#239b56;--green-200:#1d8348;--youtube:#FF0000;--musescore:#1f74bd}*,*::before,*::after{box-sizing:border-box}html{line-height:normal}body{font-family:roboto,sans-serif;min-height:100vh;scroll-behavior:smooth;text-rendering:optimizeSpeed;background-color:var(--bg-main);color:var(--text-colour);margin:0;padding:0}h1{font-size:2rem}h2{font-size:1.5rem}h3{font-size:1.17rem}h4{font-size:1rem}h5{font-size:.83rem}h6{font-size:.67rem}h1{margin:.67em 0}a:not([class]){text-decoration-skip-ink:auto;background-color:var(--surface-0);color:var(--text-colour)}a:not([class]):hover{background-color:var(--surface-1)}input,button,textarea,select{font:inherit}pre{white-space:pre-wrap}img,svg,video,canvas,audio,iframe,embed,object{vertical-align:middle;max-width:100%}img,svg,video,canvas{height:auto}.btn{border:none;cursor:pointer;padding:4px;border-radius:4px}.card{display:flex;flex-direction:column;background-color:var(--surface-0);align-items:center;justify-content:center;border-radius:10px;cursor:pointer;max-width:300px;transition:all .3s ease}.card .card-image-container{overflow:hidden}.card .card-image{width:100%;transition:transform .4s ease}.card-link{color:var(--text-colour);text-decoration:none}.card:hover{background-color:var(--surface-1)}.card:hover img{transform:scale(1.2)}.container{display:flex;padding:.5em;margin:0;align-items:center;justify-content:center}.grid{display:grid;grid-template-columns:1fr;grid-template-rows:auto;gap:10px}header{text-align:center;margin:2em}h1 a.title{font-size:3rem;color:var(--green-100);text-decoration:none;transition:all .3s ease}h1 a.title:hover{color:var(--green-50)}.navigation ul{display:flex;list-style-type:none;margin:0;padding:0}.navigation li{margin:0 2em}.navigation a{background-color:var(--surface-0);color:var(--text-colour);text-decoration:none;padding:.5rem;transition:all .3s ease}.navigation a:hover{background-color:var(--surface-1)}.navigation a.youtube{background-color:var(--youtube)}.navigation a.musescore{background-color:var(--musescore)}body{display:flex;flex-direction:column;align-items:center;justify-content:center;margin:0;padding:0}a.pdf-link{background-color:var(--surface-0);color:var(--text-colour-sub);transition:all .3s ease}a.pdf-link:hover{color:var(--text-colour)}a.musescore-link{background-color:var(--musescore);color:var(--text-colour-sub);transition:all .3s ease}a.musescore-link:hover{color:var(--text-colour)}